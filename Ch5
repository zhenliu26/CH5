#function to determine whether the input number is even
def deteven(x):
    if x%2==0:
        print x,("is even.")
    else:
        print x,("is odd.")


def ifequal(x,y):
    if x>y:
        print x,("is greater than"),y
    elif x<y:
        print x,("is less than"),y
    else:
        print x,("is equal to"),y

#Recursion
def countdown(n):
    if n==0: # base case
        print("Blastoff")
    else:
        print(n)
        countdown(n-1)

#Return
def cube(x):
    return x*x*x
print(cube(3))

def sumInt(num):
    if num==0:
        return 0
    else:
        sum=num+sumInt(num-1)
    return sum
print(sumInt(4))
